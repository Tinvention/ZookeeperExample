<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{10}.%method [%file:%line] - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="net.tinvention.zookeeper" level="INFO">
		<appender-ref ref="STDOUT" />
	</logger>

	<root level="OFF" />
</configuration>