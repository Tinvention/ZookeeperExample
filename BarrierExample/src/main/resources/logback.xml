<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern><pattern>%d{yyyy-MM-dd_HH:mm:ss.SSS} %level [%thread] %logger{10}.%method [%file:%line] - %msg%n</pattern></pattern>
		</encoder>
	</appender>

	<logger name="net.tinvention.zookeeper" level="DEBUG">
		<appender-ref ref="STDOUT" />
	</logger>

	<root level="OFF" />
</configuration>
